{
  "name": "fitchallenge",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --selectProjects unit",
    "test:unit": "jest --selectProjects unit",
    "test:coverage": "jest --selectProjects unit --coverage",
    "test:integration": "jest --selectProjects integration",
    "test:integration:critical": "jest --selectProjects integration --testPathPatterns=critical",
    "test:integration:full": "jest --selectProjects integration",
    "test:all": "jest",
    "test:ci": "jest --selectProjects unit && jest --selectProjects integration --testPathPatterns=critical",
    "e2e:seed": "npx tsx e2e/scripts/seed-users.ts",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:ios:release": "detox build --configuration ios.sim.release",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:build:android:release": "detox build --configuration android.emu.release",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:ios:release": "detox test --configuration ios.sim.release",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:test:android:release": "detox test --configuration android.emu.release",
    "e2e:smoke:ios": "detox test --configuration ios.sim.debug --testNamePattern 'Happy Path'",
    "e2e:smoke:android": "detox test --configuration android.emu.debug --testNamePattern 'Happy Path'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@expo-google-fonts/plus-jakarta-sans": "^0.4.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-google-signin/google-signin": "^16.1.1",
    "@sentry/react-native": "~7.2.0",
    "@supabase/supabase-js": "^2.49.1",
    "@tanstack/query-async-storage-persister": "^5.64.2",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-query-persist-client": "^5.64.2",
    "aes-js": "^3.1.2",
    "babel-preset-expo": "~54.0.9",
    "expo": "~54.0.0",
    "expo-apple-authentication": "~8.0.8",
    "expo-asset": "~12.0.12",
    "expo-constants": "~18.0.12",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-device": "^8.0.10",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "^17.0.8",
    "expo-notifications": "^0.32.16",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^2.0.0",
    "react-native-heroicons": "^4.0.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets-core": "^1.6.2",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/aes-js": "^3.1.4",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.10",
    "@types/uuid": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.47.0",
    "detox-cli": "^20.47.0",
    "eslint": "^9.39.2",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-expo": "~54.0.0",
    "prettier": "^3.8.1",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-test-renderer": "19.1.0"
  },
  "private": true
}
